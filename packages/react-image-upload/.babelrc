{
  "plugins": [
    ["transform-runtime"]
  ],
  "presets": [
    [
      "@oigroup/babel-preset-lightscript",
      {
        "env": {
          "targets": {
            "ie": 10
          }
        },
        "noEnforcedSubscriptIndentation": true,
        "bangCall": true,
        "safeCall": true,
        "existential": true,
        "enhancedComprehension": true
      }
    ]
  ],
  "env": {
    "istanbul": {
      "plugins": [
        ["transform-runtime"],
        ["istanbul"]
      ],
      "presets": [
        [
          "@oigroup/babel-preset-lightscript",
          {
            "env": {
              "targets": {
                "ie": 10
              }
            },
            "noEnforcedSubscriptIndentation": true,
            "bangCall": true,
            "safeCall": true,
            "existential": true,
            "enhancedComprehension": true
          }
        ]
      ]
    }
  }
}
